@keyframes fadeOut
  0%
    opacity 1
  100%
    opacity 0

@keyframes fadeIn
  0%
    opacity 0
  100%
    opacity 1

@keyframes carHide
  0%
    opacity 1
    transform scale(1) translateY(0)
  100%
    opacity 0
    transform scale(0) translateY(100%)

@keyframes carShow
  0%
    opacity 0
    transform scale(0) translateY(100%)
  100%
    opacity 1
    transform scale(1) translateY(0)


@keyframes pulse
  0%
    transform scale(0.98)
  50%
    transform scale(1.05)
  100%
    transform scale(0.98)

@keyframes pulse2
  0%
    transform scale(1)
  50%
    transform scale(0.95)
  100%
    transform scale(1)


