      //animation 0.5s fadeOut
    &.ng-hide-remove, &.ng-enter
      animation 0.5s fadeIn

    &.hide-animation
      &.ng-hide-add, &.ng-leave
        animation 0.5s fadeOut

    &.remove-animation
      &.ng-hide-add, &.ng-leave
        animation none
      &.ng-hide-remove, &.ng-enter
        animation none

    &.carrossel-animation
      &.ng-hide-add, &.ng-leave
        position absolute
        left 0
        top 0
        animation 0.3s carHide
      &.ng-hide-remove, &.ng-enter
        position absolute
        left 0
        top 0
        opacity 0
        animation 0.3s carShow
