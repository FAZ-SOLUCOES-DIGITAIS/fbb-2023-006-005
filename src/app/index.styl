      transform: translateX(-50%) translateY(calc(3rem))
      bottom: auto

    .rb-{$wid}
      top: percentage($wid / 100)
      left: 50%
      transform: translateX(-50%) translateY(calc(3rem))
      bottom: auto

    .lt-{$wid}
